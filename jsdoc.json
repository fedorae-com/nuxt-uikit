{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "includeDate": false
    }
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "template": "./template/",
    "readme": "./README.md"
  }
}
